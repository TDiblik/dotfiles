[user]
	name = YOUR_INFO
	email = YOUR_@_EMAIL
	signingkey = YOUR_KEY
[core]
	editor = \"/usr/local/bin/code\" --wait 
	ignorecase = false
	autocrlf = true
[http]
	sslVerify = true
[commit]
	gpgsign = true
[alias]
	llog = log --oneline
	sstatus = status -s
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = master
[gpg]
	program = gpg
